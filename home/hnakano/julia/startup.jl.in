let
    custom_envvars = Dict(
        "JUPYTER" => "@jupyterCmdFHS@/bin/jupyter-command",
        "PYTHON" => "@pyCallEnv@/bin/python",
        "PYTHONPATH" => "@pyCallEnv@/@sitePackages@",
    )

    for (k, v) in pairs(custom_envvars)
        haskey(ENV, k) || (ENV[k] = v)
    end
end

atreplinit() do repl
    try
        @eval using OhMyREPL
    catch e
        @warn "error while importing OhMyREPL" e
    end
end

using Revise
